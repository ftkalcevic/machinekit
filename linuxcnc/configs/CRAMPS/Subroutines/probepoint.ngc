o<probepoint> sub 
    #<x> = [#1 - #<_ProbeTipOffsetX>]
    #<y> = [#2 - #<_ProbeTipOffsetY>]

    G0 Z[#<_ZHeight>]
    G0 X[#<x>] Y[#<y>]
    G38.2 Z0 F[#<_ProbeFeedRate>]
    G0 Z[#<_ZHeight>]

    o1010 if [ #5070 EQ 1 ]
        #<z> = [#5063 + #<_ProbeTipOffsetZ>]
        ;(print, probe #<x>,#<y>,  #5063, #<z>)
    o1010 endif
o<probepoint>  endsub
M2

